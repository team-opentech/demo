{
  "name": "my-qwik-basic-starter",
  "description": "Recommended for your first Qwik app",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "trustedDependencies": [
    "sharp"
  ],
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/netlify-edge/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "netlify deploy --build",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint src/**/*.ts*",
    "preview": "qwik build preview && vite preview --open",
    "qwik-speak-extract": "qwik-speak-extract --supportedLangs=es-US,en-US --assetsPath=i18n",
    "serve": "wrangler pages dev ./dist",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/dev-tools": "^1.0.17",
    "@builder.io/qwik": "^1.5.7",
    "@builder.io/qwik-city": "^1.5.7",
    "@builder.io/qwik-react": "0.5.0",
    "@builder.io/sdk-qwik": "^0.14.26",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.15.20",
    "@mui/x-data-grid": "^6.20.1",
    "@netlify/edge-functions": "^2.8.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.14.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/scrollreveal": "^0.0.9",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-qwik": "^1.5.7",
    "netlify-cli": "^15.11.0",
    "postcss": "^8.4.38",
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "qwik-speak": "^0.17.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "scrollreveal": "^4.0.9",
    "tailwindcss": "^3.4.4",
    "typescript": "5.4.5",
    "undici": "*",
    "vite": "^5.3.1",
    "vite-tsconfig-paths": "4.2.1",
    "wrangler": "^3.61.0"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.19",
    "@heroicons/react": "^2.1.4",
    "graphql": "^16.8.2",
    "lottie-react": "^2.4.0",
    "lottie-web": "^5.12.2",
    "moment": "^2.30.1",
    "og-img": "^0.2.1",
    "swiper": "^11.1.4"
  }
}
